<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Zhen Liu</Authors>
    <NeutralLanguage>en-NZ</NeutralLanguage>
    <PackageLicenseUrl>https://github.com/touchjet/JsonRpc20.Client/blob/master/LICENSE</PackageLicenseUrl>
    <Owners>Touchjet Limited</Owners>
    <PackageProjectUrl>https://github.com/touchjet/JsonRpc20.Client</PackageProjectUrl>
    <Title>Json-Rpc 2.0 Client</Title>
    <PackageId>JsonRpc20.Client</PackageId>
    <PackageVersion>1.0.0</PackageVersion>
    <ReleaseVersion>1.0.0</ReleaseVersion>
    <SynchReleaseVersion>false</SynchReleaseVersion>
    <PackOnBuild>true</PackOnBuild>
    <PackageReleaseNotes>Initial release.</PackageReleaseNotes>
    <PackageTags>JsonRpc</PackageTags>
    <Description>A C# implementation of Json-RPC 2.0 client.</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>
</Project>
